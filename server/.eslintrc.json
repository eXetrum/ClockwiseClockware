{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": ["standard", "../.eslintrc.json"],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": ["prettier"],
    "rules": {
        "semi": "off",
        "space-before-function-paren": "off",
        "indent": ["warn", 4]
    }
}
